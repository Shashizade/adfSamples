{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "adfbyexample2025-test"
		},
		"ls_ABS_adfbyexample_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'ls_ABS_adfbyexample'"
		},
		"ls_AzureSqlDatabase_password": {
			"type": "secureString",
			"metadata": "Secure string for 'password' of 'ls_AzureSqlDatabase'"
		},
		"AnyBlobStorage_properties_typeProperties_connectionString_secretName": {
			"type": "string",
			"defaultValue": "@linkedService().ConnectionSecretName"
		},
		"ls_ABS_KV_ADFByExample_properties_typeProperties_connectionString_secretName": {
			"type": "string",
			"defaultValue": "StorageAccountConnectionString"
		},
		"ls_AKV_Adfbyexample_properties_typeProperties_baseUrl": {
			"type": "string",
			"defaultValue": "https://kv-adfbyexample2025.vault.azure.net/"
		},
		"ls_AzureSqlDatabase_properties_typeProperties_server": {
			"type": "string",
			"defaultValue": "adfbyexample2025-sql.database.windows.net"
		},
		"ls_AzureSqlDatabase_properties_typeProperties_database": {
			"type": "string",
			"defaultValue": "adfbyexample"
		},
		"ls_AzureSqlDatabase_properties_typeProperties_userName": {
			"type": "string",
			"defaultValue": "sqladmin"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "adfbyexample2025-test_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2024-03-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"ls_ABS_adfbyexample_connectionString": {
						"value": "[parameters('ls_ABS_adfbyexample_connectionString')]"
					},
					"ls_AzureSqlDatabase_password": {
						"value": "[parameters('ls_AzureSqlDatabase_password')]"
					},
					"ls_AKV_Adfbyexample_properties_typeProperties_baseUrl": {
						"value": "[parameters('ls_AKV_Adfbyexample_properties_typeProperties_baseUrl')]"
					},
					"ls_AzureSqlDatabase_properties_typeProperties_server": {
						"value": "[parameters('ls_AzureSqlDatabase_properties_typeProperties_server')]"
					},
					"ls_AzureSqlDatabase_properties_typeProperties_database": {
						"value": "[parameters('ls_AzureSqlDatabase_properties_typeProperties_database')]"
					},
					"ls_AzureSqlDatabase_properties_typeProperties_userName": {
						"value": "[parameters('ls_AzureSqlDatabase_properties_typeProperties_userName')]"
					},
					"AnyBlobStorage_properties_typeProperties_connectionString_secretName": {
						"value": "[parameters('AnyBlobStorage_properties_typeProperties_connectionString_secretName')]"
					},
					"ls_ABS_KV_ADFByExample_properties_typeProperties_connectionString_secretName": {
						"value": "[parameters('ls_ABS_KV_ADFByExample_properties_typeProperties_connectionString_secretName')]"
					}
				}
			},
			"dependsOn": []
		},
		{
			"name": "adfbyexample2025-test_ArmTemplate_1",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2024-03-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_1.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					}
				}
			},
			"dependsOn": [
				"[resourceId('Microsoft.Resources/deployments', 'adfbyexample2025-test_ArmTemplate_0')]"
			]
		}
	]
}