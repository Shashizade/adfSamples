		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "adfbyexample2025-test"
		},
		"ls_ABS_adfbyexample_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'ls_ABS_adfbyexample'"
		},
		"ls_AzureSqlDatabase_password": {
			"type": "secureString",
			"metadata": "Secure string for 'password' of 'ls_AzureSqlDatabase'"
		},
		"ls_AKV_Adfbyexample_properties_typeProperties_baseUrl": {
			"type": "string",
			"defaultValue": "https://kv-adfbyexample2025.vault.azure.net/"
		},
		"AnyBlobStorage_properties_typeProperties_connectionString_secretName": {
			"type": "string",
			"defaultValue": "@linkedService().ConnectionSecretName"
		},
		"ls_ABS_KV_ADFByExample_properties_typeProperties_connectionString_secretName": {
			"type": "string",
			"defaultValue": "StorageAccountConnectionString"
		},
		"ls_AzureSqlDatabase_properties_typeProperties_server": {
			"type": "string",
			"defaultValue": "adfbyexample2025-sql.database.windows.net"
		},
		"ls_AzureSqlDatabase_properties_typeProperties_database": {
			"type": "string",
			"defaultValue": "adfbyexample"
		},
		"ls_AzureSqlDatabase_properties_typeProperties_userName": {
			"type": "string",
			"defaultValue": "sqladmin"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
